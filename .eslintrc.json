{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
      "complexity": ["warn", 0],
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "off"
    },
    "overrides": [
      {
        "files": ["*.ts", "*.tsx"],
        "rules": {
          "complexity": ["warn", 0]
        }
      }
    ]
  }